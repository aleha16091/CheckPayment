{
  "name": "Проверка покупок",
  "description": "По всем ошибкам и вопросам можете писать в телегу @alex16091",
  "version": "2.3.0",
  "icons": {
    "32": "images/icon32x32.png",
    "48": "images/icon64x64.png"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [ "https://vk.com/*" ],
      "js": ["js/content-script.js"],
      "css": [ "css/insert.css" ],
      "run_at": "document_end"
    }
  ],
  "permissions": [ "tabs", "storage" ],
  "background": {"service_worker": "js/background.js"},
  "web_accessible_resources": [
    {
      "resources": [ "js/insert.js", "/images/Not-search-course.png", "/images/Not-search-mastergroup.png"],
      "matches": [ "https://vk.com/*" ]
    }
  ],
  "externally_connectable": {
    "matches": ["https://vk.com/*"]
  },
  "action": {
    "default_icon": {
      "32": "images/icon32x32.png",
      "48": "images/icon64x64.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "Проверка покупок"
  }
}